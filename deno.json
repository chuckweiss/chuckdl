{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "start": "deno run --allow-run --allow-read --allow-write --allow-net --allow-env --env-file=.env ./main.ts"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/path": "jsr:@std/path@^1.1.2",
    "discordeno": "npm:discordeno@^21.0.0"
  },
  "lock": false
}
